<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.cattaka.android.fastchecklist"
    android:versionCode="1"
    android:versionName="1.0.0" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyAppliction"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" android:theme="@style/MyTheme">
        <activity
            android:label="@string/app_name"
            android:name=".FastCheckListActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckListEntryActivity"></activity>
        <activity android:name=".CheckListCheckActivity"></activity>
<!--
        <service android:name=".TelnetSqliteService"></service>
 -->
    </application>
</manifest>